<div class="col-md-6 col-md-offset-3">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">Edit Session</h3>
    </div>
    <div class="panel-body">
      <%= form_for @device_session, url: device_session_path(@device, @device_session), html: { class: 'form-horizontal' } do |f| %>
        <%= render partial: 'form', locals: { f: f, submit_text: "Update Session" } %>
      <% end %>
    </div>
  </div>
</div>
