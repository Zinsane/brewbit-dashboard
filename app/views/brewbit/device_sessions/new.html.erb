<div class="col-md-6 col-md-offset-3">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">Start New Session</h3>
    </div>
    <div class="panel-body">
      <%= form_for @device_session, url: device_sessions_path, html: { class: 'form-horizontal' } do |f| %>
        <%= render partial: 'form', locals: { f: f, submit_text: "Start Session" } %>
      <% end %>
    </div>
  </div>
</div>
